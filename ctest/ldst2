 riscv32-unknown-elf-gcc -march=rv32i -mabi=ilp32 -mstrict-align -mpreferred-stack-boundary=4 -c -o ldst_test2.o ldst_test2.c
 riscv32-unknown-elf-ld -b elf32-littleriscv start.o ldst_test2.o -T link.ld -o ldst_test2
 riscv32-unknown-elf-objdump -b elf32-littleriscv -D ldst_test2 > ldst_test2.elf.dump
 riscv32-unknown-elf-objcopy -O binary ldst_test2 ldst_test2.bin
 od -An -tx4 -w4 -v ldst_test2.bin > ldst_test2.hex
